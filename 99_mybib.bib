doi = {10.1016/j.cma.2016.12.007},
file = {:C$\backslash$:/Users/t.kuipers/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wang, Chen, Wang - 2017 - Concurrent design with connectable graded microstructures.pdf:pdf},
issn = {00457825},
