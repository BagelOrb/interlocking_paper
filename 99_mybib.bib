file = {:C$\backslash$:/Users/t.kuipers/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wu et al. - 2018 - Infill Optimization for Additive Manufacturing-Approaching Bone-Like Porous Structures.pdf:pdf},
