doi = {10.1016/S0734-743X(00)00060-9},
file = {:C$\backslash$:/Users/t.kuipers/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Avalle, Belingardi, Montanini - 2001 - Characterization of polymeric structural foams under compressive impact loading by means of energ.pdf:pdf},
