doi = {10.1016/j.cad.2018.12.001},
file = {:C$\backslash$:/Users/t.kuipers/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Jaiswal, Rai - 2019 - A geometric reasoning approach for additive manufacturing print quality assessment and automated model correction.pdf:pdf},
